config_version=5

[application]

config/name="MikuVRMMotion"
run/main_scene="res://miku_miku_dance_vrm/scenes/scene.tscn"
config/features=PackedStringArray("4.5", "Double Precision")
config/icon="res://icon.png"

[autoload]

StandardBones="*res://addons/vmd_motion/common/StandardBones.gd"

[debug]

gdscript/warnings/enable=false

[display]

window/vsync/vsync_mode=false

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PackedStringArray("res://addons/Godot-MToon-Shader/plugin.cfg", "res://addons/vrm/plugin.cfg", "res://addons/vmd_motion/plugin.cfg")

[gui]

